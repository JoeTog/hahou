//自动升级
+ (id)checkLastVersionParser:(NSData*)data
{
    NSDictionary *bodyDic = [self gotDataParser:data];
    
    if (bodyDic)
    {
        if ([bodyDic objectForKey:kWrongDlog])
        {
            return nil;
        }
        return bodyDic;
    }
    
    return nil;
}

