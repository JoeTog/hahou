{
    NSDictionary * bodyDic = [self gotDataParser:data];
    if (bodyDic)
    {
        NSMutableArray *muArr = [[NSMutableArray alloc]initWithCapacity:10];
        NSArray * remindArr = [bodyDic objectForKey:@"competitionMapList"];
        
        for (NSDictionary * dic in remindArr)
        {
            competitionEntity * entity = [[competitionEntity alloc]init];
            entity.eventId = [[dic objectForKey:@"eventId"] description];
            entity.eventName = [[dic objectForKey:@"eventName"] description];
            entity.eventTime = [[dic objectForKey:@"eventTime"] description];
            entity.areaAddress = [[dic objectForKey:@"areaAddress"] description];
            entity.explain = [[dic objectForKey:@"explain"] description];
            if (entity.explain.length == 0)
            {
                entity.explain = @"  ";
            }
            entity.projectName = [[dic objectForKey:@"projectName"] description];
            entity.eventStatus = [[dic objectForKey:@"eventStatus"] description];
            entity.picPath = [[dic objectForKey:@"picPath"] description];
            entity.orgName = [[dic objectForKey:@"orgName"] description];
            [muArr addObject:entity];
        }
        
        return muArr;
    }
    return nil;
}

