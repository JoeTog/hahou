            entity.photoMapList = [bodyDic objectForKey:@"photoMapList"];
            entity.longitude = [[dic objectForKey:@"longitude"] description];
            entity.latitude = [[dic objectForKey:@"latitude"] description];
        }
        
        return entity;
    }
    return nil;
}

@end
