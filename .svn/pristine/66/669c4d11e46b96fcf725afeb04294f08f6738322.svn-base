//钱包主页信息
+ (id)getMyWalletInfoParser:(id)data
{
    NSDictionary *bodyDic = [self gotDataParser:data];
    
    if (bodyDic)
    {
        if ([bodyDic objectForKey:kWrongDlog]) {
            return nil;
        }
        
        NSDictionary * entiDic = [bodyDic objectForKey:@"myMoney"];
        MyWalletEntity * entity = [[MyWalletEntity alloc]init];
//        entity.accountBalance = [entiDic objectForKey:@"accountBalance"];
        entity.walletId = [entiDic objectForKey:@"id"];
        entity.actTotalBalance = [entiDic objectForKey:@"actTotalBalance"];
        entity.isTelephoneBinding = [entiDic objectForKey:@"isTelephoneBinding"];
        entity.isGustUnlockPswUsed = [entiDic objectForKey:@"isGustUnlockPswUsed"];
        entity.gustureUnlockPsw = [entiDic objectForKey:@"gustureUnlockPsw"];
        entity.isPayPswUsed = [entiDic objectForKey:@"isPayPswUsed"];
        
        entity.actRealBalance = [entiDic objectForKey:@"actRealBalance"];
        entity.actGiveBalance = [entiDic objectForKey:@"actGiveBalance"];
        
        return entity;
    }
    
    return nil;
}

//消费明细
+ (id)costDetailListParser:(id)data
{
    NSDictionary *bodyDic = [self gotDataParser:data];
    
    if (bodyDic)
    {
        if ([bodyDic objectForKey:kWrongDlog]) {
            return nil;
        }
        
        NSArray * cosumeArr = [bodyDic objectForKey:@"myMoneyTransDetailList"];
        NSMutableArray * retMuarr = [[NSMutableArray alloc]initWithCapacity:0];
        for (NSDictionary * dic in cosumeArr) {
            CosumeEntity * entity = [[CosumeEntity alloc]init];
            entity.transTotalAmt = [dic objectForKey:@"transTotalAmt"];
            entity.transDate = [dic objectForKey:@"transDate"];
            entity.transWay = [dic objectForKey:@"transWay"];
            entity.transRemark = [dic objectForKey:@"transRemark"];
            entity.createDate = [dic objectForKey:@"createDate"];
            entity.transTypeName = [dic objectForKey:@"transTypeName"];
            
            [retMuarr addObject:entity];
        }
        
        return retMuarr;
    }
    
    return nil;
}

