//获取支付方式
+ (id)getRechargeTypeParser:(id)data
{
    NSDictionary *bodyDic = [self gotDataParser:data];
    
    if (bodyDic)
    {
        if ([bodyDic objectForKey:kWrongDlog]) {
            return nil;
        }
        
        NSArray * cosumeArr = [bodyDic objectForKey:@"payTypeList"];
        NSMutableArray * retMuarr = [[NSMutableArray alloc]initWithCapacity:0];
        for (NSDictionary * dic in cosumeArr) {
            SportsPaymentTypeEntity * entity = [[SportsPaymentTypeEntity alloc]init];
            entity.typeId = [dic objectForKey:@"paytypeId"];
            entity.type = [dic objectForKey:@"payType"];
            entity.name = [dic objectForKey:@"payName"];
            entity.remark = [dic objectForKey:@"remark"];
            entity.picPath = [dic objectForKey:@"picPath"];
            entity.sortNum = [dic objectForKey:@"sortNum"];
            
            entity.payAddress = [dic objectForKey:@"payAddress"];
            entity.backendURL = [dic objectForKey:@"backendURL"];
            
            entity.frontendUrl = [dic objectForKey:@"frontendUrl"];
            entity.sign = [dic objectForKey:@"frontendUrl"];
            entity.versionNum = [dic objectForKey:@"versionNum"];
            
            [retMuarr addObject:entity];
        }
        
        return retMuarr;
    }
    
    return nil;
}

