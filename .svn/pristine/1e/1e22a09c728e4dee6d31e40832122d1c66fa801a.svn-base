#import "NFMatchManager.h"

@implementation NFMatchManager

//赛事首页
- (void)matchInfoManager
{
    __strong NSDictionary *infoDic = [_argList objectAtIndex:0];
    
    //获取request对象-----网络层构建
    requestGetter = ^ {
        NSURLRequest *request = [NFMatchRequest gotRequest:infoDic andURL:matchInfoUrl];
        
        return request;
    };
    
    //网络数据解析为结构化数据----解析层
    handler = ^(NSURLResponse* response, NSData* data, NSError* connectionError) {
        id bizData = [NFMatchParser matchInfoParser:data];
        return bizData;
    };
}

