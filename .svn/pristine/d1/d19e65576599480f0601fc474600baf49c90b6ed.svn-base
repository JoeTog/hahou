-(UIView *)tableView:(UITableView *)tableView viewForHeaderInSection:(NSInteger)section{
    if (section == 0) {
        UIView *messageV = [[[NSBundle mainBundle]loadNibNamed:@"MessageView" owner:nil options:nil] firstObject];
        messageV.frame = CGRectMake(0, 0, SCREEN_WIDTH, 50);
        
        return messageV;
    }
    return nil;
}

