//获取用户权限
- (void)UserRolesManager
{
    __strong NSDictionary *infoDic = [_argList objectAtIndex:0];
    
    //获取request对象-----网络层构建
    requestGetter = ^ {
        NSURLRequest *request = [NFActivityRequest gotRequest:infoDic andURL:UserRolesUrl];
        
        return request;
    };
    
    //网络数据解析为结构化数据----解析层
    handler = ^(NSURLResponse* response, NSData* data, NSError* connectionError) {
        id bizData = [NFActivityParser UserRolesParser:data];
        return bizData;
    };
}

