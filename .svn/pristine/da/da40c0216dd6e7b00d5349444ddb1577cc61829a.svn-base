//首页焦点图列表
- (void)homeFocusListManager
{
    __strong NSDictionary *infoDic = [_argList objectAtIndex:0];
    
    //needCache 是是否需要缓存  needGetCache是是否需要从缓存中取出数据－下拉刷新不取缓存数据但是要更新缓存
    BOOL needCache = NO;
    BOOL needGetCache = NO;
    if (_argList.count >= 2)
    {
        needCache = YES;
        needGetCache = [[_argList objectAtIndex:1] integerValue];
    }
    
    if (needGetCache)
    {
        bizDataGetter = ^ {
            NSString *dataStr = [NFDatabaseQueue selectManagerCache:homeFocusListURL];
            NSData *strData = [dataStr dataUsingEncoding:NSUTF8StringEncoding];
            id data = [NFActivityParser homeFocusListParse:strData];
            return data;
        };
    }
    
    //获取request对象-----网络层构建
    requestGetter = ^ {
        NSURLRequest *request = [NFActivityRequest gotRequest:infoDic andURL:homeFocusListURL];
        
        return request;
    };
    
    //网络数据解析为结构化数据----解析层
    handler = ^(NSURLResponse* response, NSData* data, NSError* connectionError) {
        id bizData = [NFActivityParser homeFocusListParse:data];
        
        if (bizData && needCache)
        {
            NSString *aStr = [[NSString alloc] initWithData:data encoding:NSUTF8StringEncoding];
            [NFDatabaseQueue insertManagerCache:homeFocusListURL dataStr:aStr];
        }
        
        return bizData;
    };
}

