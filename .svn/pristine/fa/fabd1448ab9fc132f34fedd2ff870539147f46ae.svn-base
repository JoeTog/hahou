#import "NFMineManager.h"

@implementation NFMineManager


-(void)RequestManager{
    
    __strong NSDictionary *infoDic = [_argList objectAtIndex:0];
    
    //获取request对象-----网络层构建
    requestGetter = ^ {
        NSURLRequest *request = [NFMineRequest gotRequest:infoDic andURL:updateVersionURL];
        
        return request;

    };
    
    //网络数据解析为结构化数据----解析层
    handler = ^(NSURLResponse* response, NSData* data, NSError* connectionError) {
        id bizData = [NFMineParser GetRequestParser:data];
        return bizData;
    };
    
}


@end
