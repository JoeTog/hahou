/*!
 @function
 @abstract      用户对象的实体单例
 
 @note          该对象中的对象属性不可被多线程共享访问修改
 
 @result        返回用户的单例对象
 */
+ (instancetype)shareInstance
{
    static dispatch_once_t onceToken;
    
    dispatch_once(&onceToken, ^{
        
        userEntity = [NFUserEntity new];
        
            });
    
    return userEntity;
}

